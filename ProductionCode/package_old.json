{
  "name": "agent-supabase-test",
  "version": "1.0.0",
  "description": "Test script for Supabase agent operations",
  "type": "module",
  "main": "agent-supabase-test.js",
  "scripts": {
    "start:arbitrage": "concurrently --kill-others --prefix-colors \"cyan,magenta,yellow,green,blue,red,white\" --prefix \"[{name}]\" \"npm run agent:ai-intel\" \"npm run agent:arbitrum\" \"npm run agent:base\" \"npm run agent:polygon\" \"npm run agent:arb-analyst\" \"npm run agent:base-analyst\" \"npm run agent:poly-analyst\"",
    "agent:ai-intel": "npx eliza --character ./characters/ai-prediction-intelligence-specialist.character.json --port 3001",
    "agent:arbitrum": "npx eliza --character ./characters/arbitrum-flash-specialist.character.json --port 3002",
    "agent:base": "npx eliza --character ./characters/base-speed-demon.character.json --port 3003",
    "agent:polygon": "npx eliza --character ./characters/polygon-micro-king.character.json --port 3004",
    "agent:arb-analyst": "npx eliza --character ./characters/arbitrum-quality-analyst.character.json --port 3005",
    "agent:base-analyst": "npx eliza --character ./characters/base-efficiency-analyst.character.json --port 3006",
    "agent:poly-analyst": "npx eliza --character ./characters/polygon-precision-analyst.character.json --port 3007",
    "validate:characters": "node scripts/validate-characters.js",
    "setup:arbitrage": "npm run validate:characters && npm run start:arbitrage",
    "dev:arbitrage": "npm run setup:arbitrage",
    "stop:arbitrage": "pkill -f \"eliza.*character.*json\" || true",
    "restart:arbitrage": "npm run stop:arbitrage && sleep 2 && npm run start:arbitrage",
    "status:arbitrage": "ps aux | grep -E \"eliza.*character.*json\" | grep -v grep || echo \"No arbitrage agents running\"",
    "logs:arbitrage": "tail -f logs/arbitrage-*.log 2>/dev/null || echo \"No log files found\"",
    "test:agents": "node scripts/test-agent-communication.js",
    "monitor:performance": "node scripts/monitor-team-performance.js"
  },
  "dependencies": {
    "@elizaos/adapter-sqlite": "0.25.6-alpha.1",
    "@elizaos/client-direct": "^0.25.9",
    "@elizaos/client-telegram": "0.25.6-alpha.1",
    "@elizaos/core": "^0.25.9",
    "@elizaos/plugin-0x": "0.25.6-alpha.1",
    "@elizaos/plugin-arbitrage": "0.25.6-alpha.1",
    "@elizaos/plugin-birdeye": "0.25.6-alpha.1",
    "@elizaos/plugin-browser": "1.0.0-alpha.26",
    "@elizaos/plugin-coingecko": "0.25.6-alpha.1",
    "@elizaos/plugin-dexscreener": "0.25.6-alpha.1",
    "@elizaos/plugin-evm": "0.25.6-alpha.1",
    "@elizaos/plugin-moralis": "0.25.6-alpha.1",
    "@elizaos/plugin-news": "0.25.6-alpha.1",
    "@elizaos/plugin-solana": "0.25.6-alpha.1",
    "@elizaos/plugin-web-search": "0.25.6-alpha.1",
    "@flashbots/ethers-provider-bundle": "^1.0.0",
    "@jridgewell/trace-mapping": "^0.3.25",
    "@supabase/supabase-js": "^2.50.0",
    "@swc/core": "^1.11.24",
    "@swc/helpers": "^0.5.17",
    "@types/figlet": "^1.7.0",
    "amqplib": "^0.10.8",
    "axios": "^1.6.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "dotenv": "^16.5.0",
    "ethers": "^6.7.1",
    "figlet": "^1.8.1",
    "node-cron": "^4.0.5",
    "node-fetch": "^2.7.0",
    "node-telegram-bot-api": "^0.66.0",
    "openai": "^4.82.0",
    "pdfkit": "^0.17.1",
    "pg": "^8.11.0",
    "proper-lockfile": "^4.1.2",
    "rimraf": "^6.0.1",
    "sharp": "^0.34.2",
    "telegraf": "4.16.3",
    "uuid": "^9.0.1",
    "ws": "^8.14.0",
    "zod": "^3.22.4"
  },
  "workspaces": [
    "packages/*"
  ],
  "devDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.15.19",
    "@types/pg": "^8.10.0",
    "@types/yargs": "^17.0.32",
    "concurrently": "^9.1.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.3.2",
    "ts-node": "10.9.2",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.19.4",
    "typescript": "^5.8.3"
  }
}