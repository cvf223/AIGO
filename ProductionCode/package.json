  "name": "ai-flash-loan-arbitrage-syndicate",
  "version": "1.0.0",
  "description": "AI-powered Flash Loan Arbitrage Syndicate",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "start": "node start-syndicate.js",
    "test": "jest",
    "test:safety-foundation": "node run_safety_foundation_tests.js",
    "test:mathematical-validation": "node run_safety_foundation_tests.js",
    "test:proactive-prevention": "node run_proactive_prevention_tests.js",
    "test:proactive-comprehensive": "jest --config=jest.config.proactive.js --testPathPattern=ComprehensiveProactivePreventionTestSuite.js --verbose",
    "test:proactive-direct": "node test/DirectProactivePreventionTestRunner.js",
    "test:proactive-quick": "node test/QuickProactivePreventionTest.js",
    "validate:test-setup": "node validate_test_setup.js",
    "validate:proactive-setup": "node validate_proactive_test_setup.js",
    "test:full-validation": "node validate_test_setup.js && node run_safety_foundation_tests.js",
    "test:complete-validation": "node validate_test_setup.js && node validate_proactive_test_setup.js && node run_safety_foundation_tests.js && node run_proactive_prevention_tests.js",
    "memory:test": "node src/memory/test-persistence.js",
    "memory:backup": "node -e \"import('./src/memory/ComprehensivePersistenceLayer.js').then(m => m.comprehensivePersistence.performBackup('manual'))\"",
    "test:memory:comprehensive": "node src/memory/test-advanced-memory-comprehensive.js",
    "test:memory:integration": "node src/memory/test-integration-scenarios.js",
    "test:memory:all": "node src/memory/run-all-tests.js",
    "test:memory": "pnpm test:memory:all",
    "migrate": "node database/migrations/migration-system.js migrate",
    "migrate:status": "node database/migrations/migration-system.js status",
    "migrate:rollback": "node database/migrations/migration-system.js rollback",
    "migrate:create": "node database/migrations/migration-system.js create",
    "db:init": "node database/initialize-database.js",
    "db:init:seed": "node database/initialize-database.js --seed",
    "db:init:verify": "node database/initialize-database.js --verify",
    "security:audit": "node run-security-audit.js",
    "security:check": "npm audit",
    "security:fix": "npm audit fix"
  },
